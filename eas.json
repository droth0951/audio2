{
  "cli": {
    "version": ">= 16.13.4",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "testflight": {
      "distribution": "internal",
      "channel": "staging",
      "releaseChannel": "staging",
      "ios": {
        "buildConfiguration": "Release"
      }
    },
    "production": {
      "channel": "production",
      "releaseChannel": "production",
      "ios": {
        "buildConfiguration": "Release"
      }
    }
  },
  "submit": {
    "production": {},
    "testflight": {
      "ios": {
        "appleTeamId": "YOUR_TEAM_ID_HERE"
      }
    }
  }
}
