{
  "cli": {
    "version": ">= 16.13.4",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "ios": {
        "prebuildCommand": "npx expo prebuild --platform ios && bash scripts/fix-share-extension-plist.sh"
      }
    },
    "testflight": {
      "distribution": "internal",
      "channel": "staging",
      "releaseChannel": "staging",
      "ios": {
        "buildConfiguration": "Release"
      }
    },
    "production": {
      "channel": "production",
      "releaseChannel": "production",
      "ios": {
        "buildConfiguration": "Release",
        "prebuildCommand": "npx expo prebuild --platform ios && bash scripts/fix-share-extension-plist.sh"
      }
    }
  },
  "submit": {
    "production": {},
    "testflight": {
      "ios": {
        "appleTeamId": "YULA3L5CG5"
      }
    }
  }
}
